/* 全局 */

page {
  background: #efeff4;
  width: auto;
  font-size: 30rpx;
}

.hover {
  opacity: 0.8;
}

.iconfont {
  font-size: 50rpx;
}

.globle-cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 10;
}

.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.ellipsis2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.empty{
  margin-top: 100rpx;
  text-align: center;
  color: #8E8E8E;
}

button{
background: #fff;
font-size: 30rpx;
}
button::after{
border: none;
}