/* 全局 */

page {
  background: #efeff4;
  width: auto;
  font-size: 30rpx;
}

.hover {
  opacity: 0.8;
}

.iconfont {
  font-size: 50rpx;
}

.globle-cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 10;
}

.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.ellipsis2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.empty{
  margin-top: 100rpx;
  text-align: center;
  color: #8E8E8E;
}
.control {
  display: flex;
  align-items: center;
  background: #f0f0f0;
}
.control .iconfont{
  padding: 6rpx 20rpx;
}
.control .count{
  margin: 0 4rpx;
  padding: 16rpx;
  border-left: 1rpx solid #fff;
  border-right: 1rpx solid #fff;
}