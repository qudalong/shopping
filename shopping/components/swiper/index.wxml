<swiper class='swiper my-class' indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#FE4442" indicator-color="#ccc">
  <block wx:for="{{imgList}}" wx:key="unique">
    <swiper-item>
      <image src="{{item}}" class="slide-image" mode='aspectFill' />
    </swiper-item>
  </block>
</swiper>